{
  "name": "website-starterkit-playground",
  "private": true,
  "version": "3.2.0",
  "browserslist": "> 2%, last 2 versions, not dead",
  "homepage": "https://github.com/HAWK-GT1191/gt1191-starterkit",
  "scripts": {
    "dev": "parcel \"src/**/*.html\" --no-cache --open",
    "clean": "shx rm -rf ./.parcel-cache ./dist",
    "reset": "npm run clean && shx rm -rf ./node_modules pnpm-lock.yaml package-lock.json yarn.lock",
    "build": "npm run clean && parcel build src/*.html src/*/index.html",
    "deliver": "npm run build --no-optimize"
  },
  "target": {
    "default": {
      "distDir": "dist",
      "publicUrl": "/"
    }
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "proseWrap": "preserve",
    "singleAttributePerLine": false,
    "overrides": [
      {
        "files": [
          ".",
          "**/*.html"
        ],
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "dependencies": {
    "@fontsource-variable/rubik": "^5.2.8"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.16.1",
    "parcel": "^2.16.1",
    "posthtml-expressions": "^1.11.4",
    "posthtml-include": "^2.0.1",
    "posthtml": "^0.16.0",
    "sharp": "^0.33.5",
    "shx": "^0.4.0"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    }
  }
}